{
  "name": "wysimark-editor",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=12.8.0",
    "npm": "6.9.0",
    "yarn": ">=1.22.4"
  },
  "main": "public/build/wysimark/cjs/index.js",
  "types": "public/build/wysimark/cjs/wysimark/index.d.ts",
  "files": [
    "public/build/"
  ],
  "-- engines": "# babel breaks if we upgrade node to 13.12.0",
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
    }
  },
  "-- script": "# not necessary as path defaults to scripts but for clarity",
  "scripty": {
    "path": "scripts"
  },
  "dependencies": {
    "@emotion/cache": "10.0.29",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@fortawesome/fontawesome-common-types": "^0.2.35",
    "@fortawesome/fontawesome-pro": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/pro-light-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "delay": "^5.0.0",
    "emotion-theming": "^10.0.27",
    "is-email": "^1.0.0",
    "is-hotkey": "^0.1.4",
    "is-url": "^1.2.4",
    "kind-of": "^6.0.2",
    "lodash": "^4.17.15",
    "markdown-escapes": "^1.0.3",
    "mdast-move-images-to-root": "^1.0.0",
    "mime-types": "^2.1.26",
    "prismjs": "^1.24.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-breaks": "^1.0.3",
    "remark-disable-tokenizers": "^1.0.24",
    "remark-gfm": "^1.0.0",
    "remark-inline-links": "^5.0.0",
    "remark-parse": "^8.0.3",
    "remark-sub-super": "^1.0.19",
    "shallowequal": "^1.1.0",
    "slate": "^0.66.1",
    "slate-history": "^0.66.0",
    "slate-hyperscript": "^0.66.0",
    "slate-react": "^0.66.1",
    "stylis-plugin-extra-scope": "^0.2.1",
    "superstruct": "^0.15.2",
    "unified": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@thesunny/api2": "^2.1.14",
    "@thesunny/script-utils": "^0.1.13",
    "@types/bytes": "^3.1.1",
    "@types/capitalize": "^1.0.1",
    "@types/classnames": "^2.2.9",
    "@types/delay": "^3.1.0",
    "@types/fs-extra": "^8.1.0",
    "@types/glob": "^7.1.1",
    "@types/is-url": "^1.2.28",
    "@types/jest": "^26.0.22",
    "@types/kind-of": "^6.0.0",
    "@types/lodash": "^4.14.154",
    "@types/markdown-it": "^10.0.1",
    "@types/mime-types": "^2.1.0",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.11.1",
    "@types/nodemailer": "^6.4.0",
    "@types/prismjs": "^1.16.5",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/shallowequal": "^1.1.1",
    "@vue/compiler-sfc": "^3.1.1",
    "aws-sdk": "^2.792.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.0.0",
    "beeper": "^2.1",
    "bytes": "^3.1.1",
    "chalk": "^2.4.2",
    "cypress": "^6.8.0",
    "cypress-jest-adapter": "^0.1.1",
    "dotenv": "^8.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.27.1",
    "fs-extra": "^8.1.0",
    "jest": "^25.1.0",
    "nanoid": "^3.1.28",
    "nextjs-cors": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "pretty-bytes": "^5.6.0",
    "rollup": "^2.61.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-visualizer": "^5.5.2",
    "rollup-plugin-vue": "^6.0.0",
    "scripty": "^2.0.0",
    "sort-package-json": "^1.53.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^2.6.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "-- editor": "# build and ship editor compiled build code",
    "build:react": "ts-node --project tsconfig.ts-node.json rollup/react/build-react.ts",
    "build:standalone": "ts-node --project tsconfig.ts-node.json rollup/standalone/build-standalone.ts",
    "build:vue": "ts-node --project tsconfig.ts-node.json rollup/vue/build-vue.ts",
    "watch:dynamic": "rollup --config ./config/rollup/dynamic/rollup.config.js --watch",
    "watch:editor": "yarn watch:dynamic -n __wysimark__ -i wysimark/index.tsx -o public/build/wysimark.js",
    "analyze": "ts-node --project tsconfig.ts-node.json rollup/analyze/analyze.ts",
    "-- loader": "# build and ship loader for editor",
    "build:loader:types": "tsc --project tsconfig.loader.json",
    "build:loader:js": "rollup --config ./config/rollup/loader/rollup.config.js",
    "copy:loader:js": "cp .build/wysimark-loader-types/wysimark-lazy/index.d.ts ../wysimark-loader/index.d.ts",
    "ship:loader": "yarn build:loader:js && yarn build:loader:types && yarn copy:loader:js",
    "start:dev": "next --port 3005",
    "# lint": "yarn tsc --project ./tsconfig.json --extendedDiagnostics",
    "lint": "eslint .",
    "-- browser": "# browsers scripts",
    "open:dev": "open http://localhost:3005/",
    "-- testing": "# testing",
    "test:watch": "NODE_ENV=production jest --watch",
    "test:cypress": "yarn run cypress open",
    "--- helpers": "# yarn execution helpers",
    "ts": "babel-node --extensions=.ts",
    "link:slate": "yarn link slate && yarn link slate-react && yarn link slate-history",
    "unlink:slate": "yarn unlink slate && yarn unlink slate-react && yarn unlink slate-history",
    "add:slate:experimental": "yarn add slate@experimental slate-react@experimental slate-history@experimental"
  }
}
